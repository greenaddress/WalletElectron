    "minimist": "^1.2.0",
    "mocha": "^5.0.0",
    "rollup": "^0.55.1",
    "rollup-plugin-istanbul": "^2.0.0",
    "source-map-support": "^0.5.3",
    "spectron": "^3.6.0"
  },
  "optionalDependencies": {
    "windows-foreground-love": "^0.1.0"
